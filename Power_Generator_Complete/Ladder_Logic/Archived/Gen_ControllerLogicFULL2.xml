        <body>
          <ST>
            <xhtml:p><![CDATA[(* Convert the input appropriately to Input *)

(* Data Dictionary
E_k_1 = e[k - 1]
E_k_2 = e[k - 2]
Y_k_1 = e[k - 1]
Y_k_2 = e[k - 2]
*)


U := INT_TO_REAL(omega); 
Ga := INT_TO_REAL(Gain);
U := U - 10000.0; 
U := U / 10000.0;

Y := REF - U;
ERR := REF - U;
Y2 := 1.0/tfb0 * (tfa0*E_k_1 + tfa1*E_k_2 - b1*Y_k_1 - b2*Y_k_2);
 
(* Prepare variables for next iteration *)
E_k_2 := E_k_1;
E_k_1 := ERR;
Y_k_2 := Y_k_1;
Y_k_1 := Y2;



(* Now the Algorithm Performs an Iteration*)
(* Y := C1*X1 + C2*X2 + Dd*ERR; *)
(* Xtemp := X1; *)
(* X1 := A11*X1 + A12*X2 + B1*ERR; *)
(* X2 := A21*Xtemp + A22*X2 + B2*ERR; *)

Y := Y * Ga;
Y := Y * 10000.0;
Y := Y + 10000.0;

Y2 := Y2 * Ga;
Y2 := Y2 * 10000.0;
Y2 := Y2 + 10000.0; 

act_com := REAL_TO_INT(Y);
act_com0 := REAL_TO_INT(Y2);

(* act_com := omega; *)]]></xhtml:p>
          </ST>
